{
  "compilerOptions": {
    "target": "ES2023",
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ES2023"],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
  }
}
